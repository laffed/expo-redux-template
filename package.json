    "expo-font": "~11.0.1",
    "expo-linking": "~3.3.0",
    "expo-localization": "~14.0.0",
    "expo-splash-screen": "~0.17.5",
    "expo-status-bar": "~1.4.2",
    "expo-web-browser": "~12.0.0",
    "firebase": "^9.8.3",
    "i18next": "^21.8.10",
    "immer": "^9.0.15",
    "lodash": "^4.17.21",
    "luxon": "^2.4.0",
