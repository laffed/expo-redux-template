    "husky": "^8.0.1",
    "jest": "^26.6.3",
    "jest-expo": "^47.0.0",
    "npm-run-all": "^4.1.5",
    "react-test-renderer": "17.0.1",
    "redux-devtools-extension": "^2.13.9",
    "typescript": "^4.6.3"
