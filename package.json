    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/luxon": "^2.3.2",
    "@types/react": "~18.0.24",
    "@types/react-native": "~0.70.6",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.9.0",
